@model IEnumerable<PuntoVenta.Models.Web.Producto>

@{
    ViewBag.Title = "Productos";
}

<h2>Productos</h2>

@*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
<div class="dvTblProductos">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SKU)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NOMBRE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EXISTENCIA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PRICE)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.ACTIVO)
                </th>*@
            @*<th></th>*@
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SKU)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NOMBRE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EXISTENCIA)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PRICE)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.ACTIVO)
                    </td>*@
                @*<td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.SKU }) |
                        @Html.ActionLink("Details", "Details", new { id = item.SKU }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.SKU })
                    </td>*@
            </tr>
        }

    </table>
</div>